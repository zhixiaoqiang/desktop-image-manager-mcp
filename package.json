{
  "name": "@jzone-mcp/desktop-image-manager-mcp",
  "version": "1.0.4",
  "description": "MCP 服务器，用于管理桌面图片，提供图片文件统计、查看和压缩功能",
  "main": "dist/server.js",
  "type": "module",
  "bin": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx server.ts",
    "inspector": "pnpm build & npx @modelcontextprotocol/inspector node dist/server.js"
  },
  "keywords": [
    "mcp",
    "image",
    "desktop"
  ],
  "files": [
    "dist",
    "assets"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "latest",
    "fs-extra": "^11.3.0",
    "sharp": "^0.33.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.14",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
